{
  "scriptFile": "..\\bin\\UrlShortener.Functions.dll",
  "entryPoint": "UrlShortener.Functions.RedirectHttpTrigger.Run",

  "disabled": false,
  "bindings": [
    {
      "authLevel": "anonymous",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "redirect/{shortCode}"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    }
  ]
}