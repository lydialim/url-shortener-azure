{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "RedirectUrl": {
      "matchCondition": {
        "route": "/{*shortCode}",
        "methods": [
          "GET"
        ]
      },
      "backendUri": "http://%WEBSITE_HOSTNAME%/api/redirect/{shortCode}"
    },
    "Api": {
      "matchCondition": {
        "route": "/api/{*path}"
      },
      "backendUri": "http://%WEBSITE_HOSTNAME%/api/{path}"
    }
  }
}